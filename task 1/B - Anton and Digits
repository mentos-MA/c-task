#include <bits/stdc++.h>

using namespace std;

int main()
{
    int k2;
    int k3;
    int k5;
    int k6;
    cin>>k2>>k3>>k5>>k6 ;
    int sum = 0 ;
    for(int i=0; i<4; i++)
    {
        while(k2>0 && k5>0 && k6>0 )
        {
            k2--;
            k5--;
            k6--;
            sum+=256 ;

        }
         while(k2>0 && k3>0)
            {
                k2--;
                k3--;
                sum+=32 ;
            }

    }
    cout<< sum <<endl ;
    return 0;
}
